	functions
	{
 			float adjustTotalProjectInvestment(float Total_Painting, float Total_Carpentry_Repairs, float Total_Discounts)
			{
				return Total_Painting + Total_Carpentry_Repairs - Total_Discounts;
			}
 			float adjustFinalProjectInvestment(float totalProjectInvestment, float totalAdditionalCosts)
			{
				return totalProjectInvestment + totalAdditionalCosts;
			}
 			list uploadDocument(int ID, string clientName, string projectID)
			{
				pdf_file1 = invokeurl
			[
				url :"https://creatorapp.zohopublic.com/jayleen_targetpainting/painting-contracting/record-pdf/All_Project_Specifications/" + projectID + "?privateLink=vQAd0XkwpHhNx2yqghbnEdHazpFOresJHetmSYjBSeHOgzRsPFJkUGEp8jvJf12ZKg7m6p3us0z0wYf1pnjn5ZC2etNDrvb7Pe1Q"
				type :GET
			];
				pdf_file2 = invokeurl
			[
				url :"https://creatorapp.zohopublic.com/jayleen_targetpainting/painting-contracting/record-pdf/All_Quotations1/" + ID.toString() + "?privateLink=EAAyFzbfvGax5BdhSs80VzeD5SpdYMEuCZs7G207vEfKPPQfn8m6mDJqp8tnY2wUb4VzJ6h3JbrpxAANevdyRdCGaunkt3BZznFE"
				type :GET
			];
				listFiles = List();
				listFiles.add(pdf_file1);
				listFiles.add(pdf_file2);
				dataMap = Map();
				dataMap.put("requests",{"request_name":clientName});
				requestMap = Map();
				requestMap.put("data",dataMap);
				res = zoho.sign.createDocument(listFiles,requestMap);
				myList = List();
				myList.add(res.get("requests").get("request_id").toLong());
				myList.add(res.get("requests").get("document_ids").get(0).get("document_id").toLong());
				return myList;
			}
	}



		workflow
		{
		form
		{
			Hide_Quotations as "Hide Quotations"
			{
				type =  form
				form = Project_Specifications
				record event = on add or edit

				on load
				{
					actions 
					{
						custom deluge script
						(
											hide Quotations;
										hide Lines;
										hide Final_Painting_Yes;
										hide YesSurface;
										hide Final_Painting_No;
						)
					}
				}

			}
			Adjust_Total_Painting_Pri as "Adjust Total Painting Price"
			{
				type =  form
				form = Fill_Quotations
				record event = on add or edit

				on user input of Painting.Price
				{
					actions 
					{
						custom deluge script
						(
											totalPaintingPrice = 0;
										for each  paintingRow in input.Painting
										{
											totalPaintingPrice = totalPaintingPrice + paintingRow.Price;
										}
										input.Total_Painting = totalPaintingPrice;
