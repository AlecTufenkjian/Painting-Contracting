						(
											input.Final_Painting_Yes.clear();
										for each  yesItem in input.Painting
										{
											if(yesItem.Options == "Yes")
											{
												row1 = Project_Specifications.Final_Painting_Yes();
												row1.Area=yesItem.Area;
												row1.Notes=yesItem.Notes;
												input.Final_Painting_Yes.insert(row1);
											}
										}
										for each  yesItem2 in input.Miscellaneous_Painting
										{
											if(yesItem2.Options == "Yes")
											{
												row2 = Project_Specifications.Final_Painting_Yes();
												row2.Area=yesItem2.Area;
												row2.Notes=yesItem2.Notes;
												input.Final_Painting_Yes.insert(row2);
